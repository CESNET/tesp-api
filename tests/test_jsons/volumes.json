{
    "inputs": [
        {
            "path": "/data/file_http",
            "url": "http://service-http:5000/download/test.txt",
            "type": "FILE"
        }
    ],
    "volumes": [
        "/vol/"
    ],
    "executors":[
        {
            "image":"ubuntu",
            "command":[
                "cp", "/data/file_http", "/vol/", "&&", "cat", "/vol/file_http"
            ]
        },
        {
            "image":"ubuntu",
            "command":[
                "cat", "/vol/file_http"
            ]
        }
    ]
}
